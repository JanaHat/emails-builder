{
  "type": "module",
  "name": "mjml-emails",
  "version": "1.0.0",
  "main": "index.js",
  "keywords": [],
  "author": "jana.hatasova",
  "license": "ISC",
  "description": "mjml-emails",
  "scripts": {
    "start": "nodemon src/server.js",
    "start:clientA": "CLIENT=clientA nodemon src/server.js",
    "start:clientB": "CLIENT=clientB nodemon src/server.js",
    "build": "node src/buildEmails.js && node src/minifyUi.js",
    "minify:ui": "node src/minifyUi.js",
    "build:clientA": "node src/buildEmails.js clientA",
    "build:clientA:campaignA": "node src/buildEmails.js clientA campaignA",
    "build:clientA:campaignB": "node src/buildEmails.js clientA campaignB",
    "build:clientB": "node src/buildEmails.js clientB",
    "build:all": "node src/buildEmails.js clientA && node src/buildEmails.js clientB",
    "watch:clientA": "node src/devServer.js clientA",
    "watch:clientB": "node src/devServer.js clientB",
    "dev": "concurrently -n WATCH,SERVER -c blue,green \"npm run watch:all\" \"npm run start\"",
    "dev:clientA": "concurrently -n WATCH,SERVER -c blue,green \"npm run watch:clientA\" \"npm run start:clientA\"",
    "dev:clientB": "concurrently -n WATCH,SERVER -c blue,green \"npm run watch:clientB\" \"npm run start:clientB\"",
    "watch:all": "node src/devServer.js",
    "new-template": "npm run create"
  },
  "dependencies": {
    "chokidar": "^4.0.3",
    "cssnano-preset-lite": "^4.0.4",
    "express": "^4.21.2",
    "html2canvas": "^1.4.1",
    "html-minifier-terser": "^7.2.0",
    "jszip": "^3.10.1",
    "jspdf": "^2.5.1",
    "js-beautify": "^1.15.4",
    "mjml": "^4.15.3"
  },
  "overrides": {
    "html-minifier": "npm:html-minifier-terser@7.2.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.9",
    "terser": "^5.30.3"
  }
}
